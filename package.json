    },
    {
      "path": "dist/articy-xlsx-loader.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/webpack": "^5.28.0",
    "husky": "^7.0.1",
    "memfs": "^3.2.2",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0"
  },
  "dependencies": {
    "exceljs": "^4.2.1"
  }
